[alias]
	co = checkout
	st = status -sb
        br = branch
        f = fetch
        m = merge
        r = rebase
        mom = merge origin/master
	lol = log --graph
        a = add . --all
        c = commit
        ca = commit --amend
        d = diff
[color]
	diff = auto
	status = auto
	branch = auto
[core]
	excludesfile = ~/.gitignore
	autocrlf = input
	editor = vim
[apply]
	whitespace = nowarn
[format]
	pretty = %C(yellow)%h%Creset %s %C(red)(%cr, %aN)%Creset
[push]
	default = current
[heroku]
	account = default
